<resources> 
	<properties> 
		<property id="distance" type="string">10</property>
		<property id="time" type="string">0:40:00</property>
		<property id="display" type="number">0</property>
		<property id="scalePartner" type="string">1:00:00</property>
	</properties>
	<settings>
		<setting propertyKey="@Properties.distance" title="@Strings.distanceTitle" prompt="@Strings.distanceDesc">
			<settingConfig type="alphaNumeric" />
		</setting>
		<setting propertyKey="@Properties.time" title="@Strings.timeTitle" prompt="@Strings.timeDesc">
			<settingConfig type="alphaNumeric" />
		</setting> 
		<setting propertyKey="@Properties.display" title="@Strings.displayTitle" prompt="@Strings.displayDesc">
            <settingConfig type="list">
            	<listEntry value="0">@Strings.TpsRest</listEntry>
            	<listEntry value="1">@Strings.ChronoEstimate</listEntry>
            	<listEntry value="2">@Strings.Pace</listEntry>
            </settingConfig>
        </setting>
		<setting propertyKey="@Properties.scalePartner" title="@Strings.scalePartnerTitle" prompt="@Strings.scalePartnerDesc">
			<settingConfig type="alphaNumeric" />
		</setting> 
	</settings>
</resources>		